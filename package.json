    "js-cookie": "^3.0.1",
    "jsvectormap": "1.4.5",
    "moment": "^2.29.4",
    "next": "^12.3.4",
    "next-i18next": "^12.0.1",
    "next-transpile-modules": "8.0.0",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-beautiful-dnd": "^13.1.1",
    "react-chartjs-2": "3.0.4",
    "react-color": "^2.19.3",
    "react-dom": "17.0.2",
    "react-flatpickr": "3.10.7",
    "react-i18next": "^11.18.5",
    "react-icons": "^4.10.1",
