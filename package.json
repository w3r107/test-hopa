{
  "name": "hopa-admin-menu",
  "version": "2.0.0",
  "private": true,
  "author": "Rom Raviv",
  "homepage": "https://admin.hopa.tech.com/",
  "description": "Hopa Menu",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6 <10"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start -p 8080",
    "build": "rimraf ./build && set NODE_ENV=production && next build",
    "start_deploy": "next start -p 8080",
    "lint": "next lint",
    "deploy": "gcloud config set project hopa-menu-admin && gcloud app deploy app.yaml --version=2-0-0",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "install:peer-deps": "npm install --legacy-peer-deps",
    "analyze": "ANALYZE=true yarn build"
  },
  "dependencies": {
    "@emotion/cache": "11.7.1",
    "@emotion/react": "11.8.1",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "11.8.1",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "5.4.4",
    "@mui/material": "5.4.4",
    "@next/bundle-analyzer": "^13.1.6",
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^1.5.0",
    "browser-image-compression": "^2.0.0",
    "chart.js": "^3.1.0",
    "chroma-js": "2.4.2",
    "dropzone": "5.9.2",
    "flatpickr": "4.6.9",
    "formik": "2.2.9",
    "html-to-text": "^8.2.1",
    "html5-device-mockups": "^3.2.1",
    "i18next": "^21.9.1",
